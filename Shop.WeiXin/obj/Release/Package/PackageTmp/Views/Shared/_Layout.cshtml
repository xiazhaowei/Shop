    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/csses")
    @RenderSection("Header", false)
</head>
<body>
    <div>
        @RenderBody()
    </div>
    @Scripts.Render("~/jses")
